-- movement

function is_moving_up(o)
 return o.a >= 0.5
end

function is_moving_down(o)
 return not is_moving_up(o)
end

function is_moving_right(o)
 return o.a>=0.25 and o.a<=0.75
end

function is_moving_left(o)
 return not is_moving_right(o)
end

