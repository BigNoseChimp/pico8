-- collision
f_blk=1
f_bad=2

function has_hit_enemy(o,width,height,f)
 foreach(enemies,
  function(e)
   local x=o.x
   local y=o.y
 
   if ( o.what=="ship" ) then
     x-=4
     y-=4
   end 
   x=flr(x)
   y=flr(y)
 --  o.msg=o.msg.."("..x..","..y..")"
 --  o.msg=o.msg.."("..e.x..","..e.y..")"

   if ( 
     -- leftmost o right of rightmost e
     (x > e.x+8) or
     -- rightmost o left of leftmost e
     (x+width < e.x) or
     -- topmost o below bottom e
     (y > e.y+8) or -- todo: e.height
     -- bottommost o above top e
     (y+height < e.y)
    ) then
      -- ok
  --    o.msg=o.msg.." alive"
    else
  --    o.msg=o.msg.." dead"
     f(e)
    end
  end)
 end

function x_collide(o,f)
 local l_spr=mget(
  							(o.x-o.r-1)/8,
  							o.y/8) 			
 local r_spr=mget(
 								(o.x+o.r+1)/8,
 								o.y/8)
 return (is_moving_right(o) and
      fget(r_spr,f) )
   or
        (is_moving_left(o) and
        fget(l_spr,f) )								
end


function y_collide(o,f)
 local u_spr=mget(
  							o.x/8,
  							(o.y-o.r-1)/8)
  			
 local d_spr=mget(
 							 o.x/8,
 								(o.y+o.r+1)/8)
 return (is_moving_up(o) and
      fget(u_spr,f) )
   or
        (is_moving_down(o) and
        fget(d_spr,f) )								
end

