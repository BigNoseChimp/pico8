-- explosions
explosions={}

function add_explosion(x,y)
 local e={
  x=x,
  y=y,
  s=1,
  upd=function(self)
   self.s+=1
   if (self.s>30) then
    del(explosions,self)
    kill=false
    ship.dead=false
   end
  end,
  drw=function(self)
   circ(self.x,self.y,self.s,flr(rnd(16)))
  end 
 }
 add(explosions,e)
end
