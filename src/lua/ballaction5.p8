-- trails and levels
trails={}
trail_plt={7,6,5,0}

function add_trail(t)
 local trail={
  x=t.x,
  y=t.y,
  age=0,
  drw=function(self)
   pset(self.x+rnd(4)-2,self.y+rnd(4)-2,trail_plt[flr(self.age/10)+1])
  end,
  upd=function(self)
   if(self.age > 39 ) then
    del(trails,self)
   else
    self.age+=1
   end
  end
  }
  add(trails,trail)
end

-- levels
function choose_level()
  if (ship.dead or ship.cooldown > -40 ) return
  if g.timer > 1 and g.lvl==0 then
   level(1)
--  elseif g.timer > g.lvl*8 and g.lvl>0 then
  elseif #enemies == 0 then
   level(g.lvl+1)
   add_floater(ship.x,ship.y,"l e v e l "..g.lvl)
  end 
end

function level(l)
 g.lvl=l
 for i=1,3+l do
  choose_enemy(l
           ,20+rnd(g.sizex-50)
           ,20+rnd(g.sizey-50))
 end
 if ( l%3==0) then
    choose_enemy(l
           ,20+rnd(g.sizex-50)
           ,20+rnd(g.sizey-50),true)
 end
 ship.msg="level "..l
end
