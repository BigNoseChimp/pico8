--ship
ship = {
 offset=2,
 cooldown=0,
 score=0,
 death_count=0,
 msg="",
 dead=false,
 what="ship",
 fire=0,
 x=60,
 y=60,
 r=4,
 a=0,
 turnspeed=0.025,
 p=100,
 power=0,
 spd=0,
 grav=0.15,
 acc=false,
 accplt={15,14,8},
 accelerate = function(self, b)
  self.spd-=0.02
  if b then
   self.spd += 0.04
  end
  self.acc = b
 end,
 fire_weapon = function (self,b)
  if b then
   if #bullets < 30 then
    fire_weapon(ship)
    sfx(5)
   end
  else
   self.fire=0
  end
end,
 move = function (self)
  if ( self.spd > 2 ) self.spd = 2
  if ( self.spd < 0 ) self.spd = 0
  if self.spd>0 then
   add_trail(self)
   self.x-=cos(self.a)*(self.spd)
   self.y-=sin(self.a)*(self.spd)
  end
end,
 drw=function(self)
   if not self.dead 
      and self.acc then
   sfx(flr(self.spd)+1)
   -- fillp(flr(rnd(32768)))
   circfill(self.x+cos(self.a)*self.r
           ,self.y+sin(self.a)*self.r
           ,3
           ,self.accplt[flr(self.spd)+1])
   end
  if not self.dead then
   p=0
   if ( self.fire > 0 ) then 
    p = self.fire
   end
   circfill(self.x, self.y, self.r, 7)
   circfill(self.x, self.y, self.r-1, 6)
 
  --line(self.x,self.y
  --    ,self.x+cos(self.a)*self.r
  --    ,self.y+sin(self.a)*self.r,5)
   circfill(
    self.x+cos(self.a)*-2,
    self.y+sin(self.a)*-2,
    2,p)
   end
 end,
 upd=function(self)
  self.cooldown-=1
  if self.dead then
   if self.spd > 0 then 
    self.spd-=0.1
   end
   if ( self.cooldown %3 == 0) then 
   	add_explosion(self.x,self.y,50)
   end
   if ( self.cooldown <= 0 ) then
    self.dead=false
   end
  end
  if ( self.spd > 0 ) then
   if x_collide(self,f_blk) then
    bounce_x(self,true)
   end
   if y_collide(self,f_blk) then 
    bounce_y(self,true)
   end
  end 
  self:move() 
 end
}
