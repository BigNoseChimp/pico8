-- trails
trails={}
trail_plt={7,6,5,0}

function add_trail(t)
 local trail={
  x=t.x,
  y=t.y,
  age=0,
  drw=function(self)
   pset(self.x+rnd(4)-2,self.y+rnd(4)-2,trail_plt[flr(self.age/10)+1])
  end,
  upd=function(self)
   if(self.age > 39 ) then
    del(trails,self)
   else
    self.age+=1
   end
  end
  }
  add(trails,trail)
end
