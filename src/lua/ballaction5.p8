-- bounce

function change_a(o, newa)
 o.a = newa
 if ( o.a > 1 ) o.a = o.a-1
 if ( o.a <= 0 ) o.a = 1+o.a
end

function bounce_x(o,s)
-- if (spd < 0.2 ) spd = 0.2
  change_a(o, 0.5 - o.a)
  if (s) sfx(0)
end

function bounce_y(o,s)
-- if ( spd < 0.2) spd = 0.2
  change_a(o, 1 - o.a)
  if (s) sfx(0)
end


