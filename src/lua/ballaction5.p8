-- enemy 1
-- pallette 7,15,14,8
enemies={}

plt_orig={7,15,14}
plt_chgs={
 {7,15,14},
 {15,14,8},
 {14,8,8},
 {8,8,8},
 {14,8,8},
 {15,14,8},
 {7,15,14},
 {7,7,15},
 {7,7,7}
}


function add_enemy(x,y)
 local enemy={
  x=x,
  y=y,
  r=4,
  dx=0,
  dy=0,
  sp=4,
  spmod=0,
  a=0.25,
  spd=1,
  frame=0,
  dead=false,
  upd=function(self)
   if (self.dead) then
    del(enemies,self)
    if #enemies<2 then
     _init()
    end
   end
   self.spmod=(self.spmod+1) %2
   self.dx=cos(self.a)*self.spd
   self.dy=sin(self.a)*self.spd
   if(rnd(10)<1) then
    self.frame=(self.frame+1)%9   
   end
   if ( self.spd > 0 
    and x_collide(self,f_blk) )
   then  
      bounce_x(self,false)
   elseif ( self.spd > 0 
      and y_collide(self,f_blk) ) 
   then
      bounce_y(self,false)
   else 
      self.x-=self.dx
      self.y-=self.dy
   end
  end,
  drw=function(self)
    local idx=1
   	local p=plt_chgs[self.frame%8+1]
   	for i=1,#plt_orig do
   	  pal(plt_orig[i],p[i])
    end
    spr(self.sp,
      self.x,
      self.y)
    pal()
  end
 }
 add(enemies,enemy)
end
