-- bullets
bullets={}

function fire_weapon(ship)

 local bullet = {
  what="bullet",
  x=ship.x,
  y=ship.y,
  a=ship.a,
  c=9,
  spd=4,
  r=0.1,
  dead_count=100,
  drw=function(self)
   pset(self.x,self.y,self.c)
  end,
  upd=function(self)
--   self.x=rnd(2)-1+self.x-cos(self.a)*self.spd
--   self.y=rnd(2)-1+self.y-sin(self.a)*self.spd
   local df=rnd(0.1)-0.05
   self.x=self.x-cos(self.a+df)*self.spd
   self.y=self.y-sin(self.a+df)*self.spd

   if self.new == nil then
    self.new=true
   else
    if (self.dead_count < 0) 
     or (x_collide(self,f_blk))
     or (y_collide(self,f_blk)) then
     del(bullets,self)
    else
     self.dead_count-=1 
    end
   end
   has_hit_enemy(self,0,0,
      function(e) 
       e.dead=true
       del(bullets,self)
      end )
  end
 }
 add(bullets,bullet)
end
